#include<stdio.h>

int main()
{
    double compras, valor_descontado;

    //Inserir os dados
    printf("Qual foi o valor da compra? ");
    scanf("%lf",&compras);

    //Processamento dos dados
    valor_descontado=compras*0.85;

    //Mostra o resultado
    printf("O valor da compra com desconto fica %lf reais",valor_descontado);


    return 0;
}
