#include<stdio.h>

int main ()
{
    char nome[50]; //Define a variável que irá ser utilizada, no caso o nome da pessoa
    printf("Digite seu nome: "); //Pede o nome da pessoa
    scanf("%s",nome); //É o comando para leitura do nome digitado

    printf("Bem vindo, %s!!!",nome);

    return 0;
}
