#include<stdio.h>

int main()
{
  int celsius, fahrenheit;

  //Leitura dos dados
  printf("Qual a temperatura em graus celsius? ");
  scanf("%d",&celsius);

  //Processamento 
  fahrenheit = (1.8*celsius)+32;

  //Mostra os dados
  printf("A temperatura equivalente em fahrenheit é de %d°F",fahrenheit);
    
    return 0;
}
